class Test{
    void add10(int a){
        a=a+10;
    }
}
public class ArgumentPassByValue {
    public static void main(String args[])
    {
        Test ob=new Test();
        int a=15;
        System.out.println("value of before a call:"+a);
        ob.add10(a);
        System.out.println("value after a call:"+a);
    }
    
}
